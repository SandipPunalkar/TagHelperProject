@model Speaker

    <div class="col-xl-12 col-sm-6 col-md-4 col-lg-3">
        <div class="card">
            <div class="card-img">
                <img src="~/images/@(Model.SpeakerId).jpg" />
            </div>
            <h4 class="card-title">
                <a href="#">@(Model.UserFirstName) @(Model.UserLastName)</a>
            </h4>
            <p>@(Model.Company)</p>
            <p>Session time to be announced</p>
            <ul>
                <li>
                    <a href="#" target="_blank">LinkedIn</a>
                </li>
                <li>
                    <a href="#" target="_blank">Microsoft</a>
                </li>
            </ul>
        </div>
    </div>